  "scripts": {
    "analyze": "cross-env analyze=true next build && bun report-bundle-size.js",
    "analyze:browser": "cross-env bundle_analyze=browser next build",
    "audit": "bun run prepare:audit && bunx audit-ci@^7.1.0 --critical --report-type important",
    "audit:high": "bun run prepare:audit && bunx audit-ci@^7.1.0 --high --report-type full",
    "audit:moderate": "bun run prepare:audit && bunx audit-ci@^7.1.0 --moderate --report-type summary",
    "audit:ts": "bun run ./bin/ts/audit.ts",
    "build": "tsc -b && vite build",
    "build:client": "tsc -b && vite build --outdir dist/app/client",
    "build:storybook": "storybook build",
    "check:unused": "bun run ./bin/ts/check-unused.ts",
    "chromatic": "bunx chromatic --project-token=<>",
    "copyright:ts": "bun run ./bin/ts/copyright.ts",
    "predev": "rm -rf bun.lock bun.lockb && bun install",
    "dev": "bun run audit:ts; if [ $? -eq 1 ]; then exit 1; else vite; fi",
    "docker:setup": "bash ./bin/sh/docker",
    "hash:gen": "bun run ./bin/ts/img-to-b64.ts",
    "preinstall": "bun ./bin/ts/database.gen.ts || true",
    "postinstall": "",
    "kill": "bash ./bin/sh/killports",
    "lint": "bunx biome check --write ./ || true && bunx stylelint src/styles/**/*.{css,scss,sass} --fix || true && bun run ./bin/ts/css-hsl-hex.ts",
    "lint:md": "markdownlint \"**/*.md\" \".github/**/*.md\" --rules sentences-per-line",
    "lint:packages": "pnpm dedupe --check",
    "package:cost": "bun run ./bin/ts/package-cost.ts",
    "pkg:cost": "bun run ./bin/ts/package-cost.ts",
    "prepare:audit": "bun install --yarn > yarn.lock",
    "preview": "vite preview",
    "release": "standard-version",
    "report:bundle": "bun run ./bin/ts/report-bundle-size.js",
    "rm:dupes": "bun run ./bin/ts/remove-duplicate-packages.ts",
    "storybook": "storybook dev -p 6006",
    "test:all": "bunx vitest run && cypress run && playwright test && storybook test",
    "test:cypress": "cypress run",
    "test:playwright": "playwright test",
    "test:storybook": "storybook test",
    "test:vitest": "bunx vitest run",
    "uninstall": "bun run ./bin/ts/uninstall.ts \"$@\"",
    "uninstall:ts": "bun run ./bin/ts/uninstall.ts",
    "unused": "bun run ./bin/ts/check-unused.ts"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      "defaults",
      "not dead",
      "not IE 11",
      "not IE_Mob 11",
      "maintained node versions"
    ]
  },
