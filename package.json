  "sideEffects": false,
  "type": "module",
  "main": "./dist/assets/index-*.js",
  "files": [
    "README.md",
    "AUTHORS.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "CODE_OF_CONDUCT.md",
    "SECURITY.md",
    "LICENSE"
